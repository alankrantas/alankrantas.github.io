{
	"name": "alankrantas.github.io",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"upgrade-all": "yarn-upgrade-all",
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"preview": "vite preview --port 3000",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"start": "concurrently \"npm run dev\" \"open-cli http://localhost:3000\"",
		"serve": "concurrently \"serve ./dist -l 8080\" \"open-cli http://localhost:8080\"",
		"commit": "git pull origin main && git add . && git commit -m \"Updating\" && git push origin main",
		"docker-build": "docker build . -t alankrantas.github.io -f Dockerfile",
		"docker-run": "concurrently \"docker run -d -p 8080:80 --rm alankrantas.github.io\" \"open-cli http://localhost:8080\""
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.48",
		"@sveltejs/kit": "^1.0.0-next.570",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"concurrently": "^7.6.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"open-cli": "^7.1.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"serve": "^14.1.2",
		"svelte": "^3.53.1",
		"svelte-check": "^2.10.0",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^3.2.4",
		"yarn-upgrade-all": "^0.7.1"
	},
	"type": "module",
	"dependencies": {
		"bootstrap": "^5.2.3"
	}
}
