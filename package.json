{
	"name": "alankrantas.github.io",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"upgrade-all": "yarn-upgrade-all && yarn upgrade",
		"dev": "vite dev --port 3000 --open --host",
		"build": "vite build",
		"preview": "vite preview --port 8080 --open --host",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"start": "yarn dev",
		"serve": "open-cli http://localhost:8080 && yarn preview",
		"commit": "yarn format && git pull origin main && git add . && git commit -m \"Updating\" && git push origin main",
		"docker-build": "docker build . -t alankrantas.github.io -f Dockerfile",
		"docker-run": "open-cli http://localhost:8080 && docker run -d -p 8080:80 --rm alankrantas.github.io"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.1",
		"@sveltejs/kit": "^1.8.4",
		"@typescript-eslint/eslint-plugin": "^5.53.0",
		"@typescript-eslint/parser": "^5.53.0",
		"eslint": "^8.34.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"open-cli": "^7.1.0",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.4",
		"svelte-preprocess": "^5.0.1",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.4",
		"yarn-upgrade-all": "^0.7.2"
	},
	"type": "module",
	"dependencies": {
		"bootstrap": "^5.2.3"
	}
}
