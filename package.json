{
	"name": "alankrantas.github.io",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"start": "open-cli http://localhost:3000 && npm run dev",
		"serve": "open-cli http://localhost:8080 && serve ./dist -l 8080",
		"pull": "git pull origin main",
		"commit": "git add . && git commit -m \"Updating\" && git push origin main",
		"svelte-upgrade": "yarn upgrade svelte@latest svelte-check@latest svelte-preprocess@latest @sveltejs/kit@next @sveltejs/adapter-static@next"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.43",
		"@sveltejs/kit": "^1.0.0-next.490",
		"@typescript-eslint/eslint-plugin": "^5.38.0",
		"@typescript-eslint/parser": "^5.38.0",
		"eslint": "^8.23.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"open-cli": "^7.0.1",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"serve": "^14.0.1",
		"svelte": "^3.50.1",
		"svelte-check": "^2.9.0",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.0",
		"typescript": "^4.8.3",
		"vite": "^3.1.3",
		"yarn-upgrade-all": "^0.7.1"
	},
	"type": "module",
	"dependencies": {
		"bootstrap": "^5.2.1"
	}
}
